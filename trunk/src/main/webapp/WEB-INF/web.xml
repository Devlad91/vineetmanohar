<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>VineetManohar.com</display-name>
  <description>Vineet Manohar.com webapp</description>

  <filter>
    <filter-name>PageExpiryFilter</filter-name>
    <filter-class>
      com.omnisee.web.PageExpiryFilter
    </filter-class>
  </filter>

  <!-- set page expiry for all pages configured -->
  <filter-mapping>
    <filter-name>PageExpiryFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>